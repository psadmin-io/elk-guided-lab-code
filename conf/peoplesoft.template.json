{
  "index_patterns": "filebeat*",
  "version": 50003,
  "settings": {
    "index.refresh_interval": "10s"
  },
  "mappings": {
    "doc": {
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "duration": {
          "type": "float"
        },
        "geoip": {
          "dynamic": "true",
          "properties": {
            "ip": {
              "type": "ip"
            },
            "location": {
              "type": "geo_point"
            },
            "latitude": {
              "type": "half_float"
            },
            "longitude": {
              "type": "half_float"
            }
          },
          "id": {
            "type": "long"
          },
          "load_done": {
            "type": "long"
          },
          "offset": {
            "type": "long"
          },
          "queued_requests": {
            "type": "long"
          },
          "requests_done": {
            "type": "long"
          },
          "seq_num": {
            "type": "long"
          },
          "servlet_timestamp": {
            "type": "date"
          }
        }
      }
    }
  }
}